<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="3000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnClick motion:targetId="@+id/imgStart" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="60dp"
            android:layout_width="60dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView3"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp"
            motion:layout_constraintHorizontal_bias="0.006"
            android:layout_marginStart="2dp" />
        <Constraint
            android:layout_height="60dp"
            android:layout_marginTop="16dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="60dp"
            android:id="@+id/imageView4"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@+id/imageView3"
            motion:layout_constraintHorizontal_bias="0.975"
            android:layout_marginEnd="0dp" />
        <Constraint
            android:layout_height="215dp"
            android:layout_width="206dp"
            android:id="@+id/imageView5"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            android:layout_marginTop="56dp"
            motion:layout_constraintHorizontal_bias="0.496"
            android:rotation="360" />
        <Constraint
            android:id="@+id/imgTua"
            motion:layout_constraintEnd_toStartOf="@+id/imgStart"
            android:layout_width="40dp"
            android:layout_height="40dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imgStart"
            motion:layout_constraintVertical_bias="0.0"
            android:layout_marginEnd="60dp"
            motion:layout_constraintTop_toTopOf="@+id/imgStart"
            android:layout_marginRight="60dp" />
        <Constraint
            android:id="@+id/imgTua"
            motion:layout_constraintEnd_toStartOf="@+id/imgStart"
            android:layout_width="40dp"
            android:layout_height="40dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imgStart"
            motion:layout_constraintVertical_bias="0.0"
            android:layout_marginEnd="60dp"
            motion:layout_constraintTop_toTopOf="@+id/imgStart"
            android:layout_marginRight="60dp" />
        <Constraint
            android:id="@+id/imgStart"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="41dp"
            android:layout_height="39dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView6"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="76dp"
            motion:layout_editor_absoluteY="610dp" />
        <Constraint
            android:id="@+id/imgStart"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="40dp"
            android:layout_height="40dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView6"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="76dp" />
        <Constraint
            android:layout_height="18dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.161"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:id="@+id/imageView6"
            motion:layout_constraintTop_toBottomOf="@+id/textView3"
            android:layout_marginTop="40dp" />
        <Constraint
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="@+id/imageView5"
            motion:layout_constraintStart_toStartOf="@+id/imageView5"
            motion:layout_constraintTop_toBottomOf="@+id/imageView5"
            android:layout_marginTop="50dp">
            <CustomAttribute
                motion:attributeName="text"
                motion:customStringValue="Love Story" />
            <CustomAttribute
                motion:attributeName="textColor"
                motion:customColorValue="@color/white" />
            <CustomAttribute
                motion:attributeName="textSize"
                motion:customPixelDimension="4dp" />
        </Constraint>
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="@+id/textView"
            android:layout_width="wrap_content"
            android:id="@+id/textView3"
            motion:layout_constraintStart_toStartOf="@+id/textView"
            motion:layout_constraintHorizontal_bias="0.545"
            motion:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginTop="16dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@+id/imageView3"
            android:layout_width="wrap_content"
            android:id="@+id/textView2"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView3"
            motion:layout_constraintStart_toEndOf="@+id/imageView3"
            motion:layout_constraintVertical_bias="0.45"
            android:layout_marginLeft="92dp"
            android:layout_marginStart="92dp" />
        <Constraint
            android:layout_height="40dp"
            android:layout_marginBottom="16dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imgStart"
            android:layout_width="40dp"
            android:id="@+id/imgStarttttt"
            motion:layout_constraintTop_toTopOf="@+id/imgStart"
            motion:layout_constraintStart_toEndOf="@+id/imgStart"
            motion:layout_constraintVertical_bias="0.0"
            android:layout_marginLeft="60dp"
            android:layout_marginStart="60dp" />
        <Constraint
            android:layout_height="40dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="40dp"
            android:id="@+id/imgStop"
            android:alpha="0"
            motion:layout_constraintTop_toBottomOf="@+id/imageView6"
            android:layout_marginTop="76dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp"
            android:id="@+id/imageView3"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.006"
            android:layout_marginStart="2dp" />
        <Constraint
            android:layout_height="60dp"
            android:layout_marginTop="16dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="60dp"
            android:id="@+id/imageView4"
            motion:layout_constraintStart_toEndOf="@+id/imageView3"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.975" />
        <Constraint
            android:id="@+id/imageView5"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="206dp"
            android:layout_height="215dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            android:layout_marginTop="56dp"
            motion:layout_constraintHorizontal_bias="0.496" />
        <Constraint
            android:id="@+id/imageView5"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="206dp"
            android:layout_height="215dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            android:layout_marginTop="56dp"
            motion:layout_constraintHorizontal_bias="0.497" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView2"
            motion:layout_constraintTop_toTopOf="@+id/imageView3"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView3"
            motion:layout_constraintStart_toEndOf="@+id/imageView3"
            android:layout_marginLeft="92dp"
            android:layout_marginStart="92dp"
            motion:layout_constraintVertical_bias="0.45" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView"
            motion:layout_constraintStart_toStartOf="@+id/imageView5"
            motion:layout_constraintEnd_toEndOf="@+id/imageView5"
            motion:layout_constraintTop_toBottomOf="@+id/imageView5"
            android:layout_marginTop="50dp" />
        <Constraint
            android:layout_height="18dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.161"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:id="@+id/imageView6"
            motion:layout_constraintTop_toBottomOf="@+id/textView3"
            android:layout_marginTop="40dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="@+id/textView"
            android:layout_width="wrap_content"
            android:id="@+id/textView3"
            motion:layout_constraintStart_toStartOf="@+id/textView"
            motion:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginTop="16dp"
            motion:layout_constraintHorizontal_bias="0.545" />
        <Constraint
            android:layout_marginEnd="60dp"
            android:layout_height="40dp"
            android:layout_marginRight="60dp"
            motion:layout_constraintTop_toTopOf="@+id/imgStart"
            motion:layout_constraintBottom_toBottomOf="@+id/imgStart"
            android:layout_width="40dp"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintEnd_toStartOf="@+id/imgStart"
            android:id="@+id/imgTua" />
        <Constraint
            android:layout_height="40dp"
            android:layout_marginTop="76dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="40dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView6"
            android:id="@+id/imgStart"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.498"
            android:alpha="0" />
        <Constraint
            android:layout_height="40dp"
            android:layout_width="40dp"
            android:id="@+id/imgStarttttt"
            motion:layout_constraintTop_toTopOf="@+id/imgStart"
            motion:layout_constraintBottom_toBottomOf="@+id/imgStart"
            motion:layout_constraintStart_toEndOf="@+id/imgStart"
            motion:layout_constraintVertical_bias="1.0"
            android:layout_marginLeft="60dp"
            android:layout_marginStart="60dp" />
        <Constraint
            android:id="@+id/imgStop"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="32dp"
            android:layout_height="32dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintTop_toBottomOf="@+id/imageView6"
            android:layout_marginTop="76dp" />
    </ConstraintSet>
</MotionScene>